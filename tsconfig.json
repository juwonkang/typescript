{
  "compilerOptions": {
    "target": "es2016",

    /* Modules */
    "module": "commonjs",

    "forceConsistentCasingInFileNames": true,

    /* Type Checking */
    "strict": true,
    "skipLibCheck": true,
    "outDir": "dist",
    "noUncheckedIndexedAccess": true
    //"lib": ["es5"]
  },
  "include": ["src/**/*"]
}
